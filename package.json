  "scripts": {
    "start": "node scripts/client.start.js",
    "build": "node scripts/client.build.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "node scripts/client.start.js",
    "prod:client": "node scripts/client.build.js",
    "dev:server": "nodemon --watch ./ --exec \"node build/server/bundle.js\"",
    "dev:build-server": "NODE_ENV=development webpack --config config/webpack.config.server.dev.js --watch",
    "prod:server": "node scripts/server.build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "node scripts/test.js --env=jsdom --coverage"
  },
  "devDependencies": {
    "csscomb": "^4.2.0",
    "tmp": "^0.0.33",
    "typescript": "^2.8.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx,ts,tsx}",
      "!client/src/**/*.{d.ts}",
      "!client/src/utils/test-utils.ts",
      "server/src/**/*.{js,jsx,ts,tsx}",
      "!server/src/**/*.{d.ts}",
      "!server/src/utils/test-utils.ts"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/enzyme.js"
    ],
    "testMatch": [
      "<rootDir>/client/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/client/src/**/?(*.)(spec|test).(j|t)s?(x)",
      "<rootDir>/server/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/server/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "proxy": {
    "/api": {
      "target": "http://188.166.109.25:8080",
      "pathRewrite": {
        "^/api": ""
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
